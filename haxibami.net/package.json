    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "plaiceholder": "^2.5.0",
    "prettier": "2.8.1",
    "sass": "1.57.1",
    "sharp": "^0.31.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
