{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "haxibami.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm lint && pnpm format && ts-node --esm hooks/before-build.mts",
    "build": "next build",
    "postbuild": "ts-node --esm hooks/after-build.mts",
    "start": "next start",
    "pw-install": "playwright install chromium",
    "ts-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --check '**/*.{ts, mts, js, tsx, json}'",
    "analyze": "ANALYZE=true next build",
    "blog": "./article.sh",
    "prepare": "cd .. && husky install haxibami.net/.husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js, ts, jsx, tsx, json}": [
      "prettier --write",
      "next lint --fix"
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.1.6",
    "@next/font": "13.1.6",
    "@vercel/og": "0.1.0",
    "feed": "4.2.2",
    "mdast-util-to-hast": "12.3.0",
    "mermaid": "9.3.0",
    "next": "13.1.6",
    "next-mdx-remote": "4.3.0",
    "next-themes": "0.2.1",
    "playwright": "1.30.0",
    "playwright-core": "1.30.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-katex": "6.0.2",
    "rehype-parse": "8.0.4",
    "rehype-pretty-code": "0.9.3",
    "rehype-raw": "6.1.1",
    "rehype-slug": "5.1.0",
    "remark-frontmatter": "4.0.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1",
    "remark-jaruby": "0.1.0",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remark-slug": "7.0.1",
    "remark-stringify": "10.0.2",
    "remark-unwrap-images": "3.0.1",
    "shiki": "0.14.1",
    "strip-markdown": "5.0.0",
    "svgo": "3.0.2",
    "unified": "10.1.2",
    "unist-util-is": "5.2.0",
    "unist-util-visit": "4.1.2",
    "vfile": "5.3.7",
    "vfile-matter": "4.0.1"
  },
  "devDependencies": {
    "@plaiceholder/next": "2.5.0",
    "@types/hast": "2.3.4",
    "@types/mdast": "3.0.10",
    "@types/mdx": "2.0.3",
    "@types/node": "18.13.0",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "fetch-site-metadata": "0.1.2",
    "globby": "13.1.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "plaiceholder": "2.5.0",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "postcss-nesting": "11.1.0",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.2",
    "sharp": "0.31.3",
    "tailwindcss": "3.2.6",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
