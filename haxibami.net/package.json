{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "haxibami.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm lint-check && pnpm format-check && node --loader ts-node/esm hooks/before-build.mts && pnpm ts-check",
    "build": "next build",
    "start": "next start",
    "pw-install": "playwright install chromium",
    "ts-check": "tsc --noEmit",
    "lint": "next lint --fix",
    "lint-check": "next lint",
    "format": "prettier --write '**/*.{ts, mts, js, tsx, json}'",
    "format-check": "prettier --check '**/*.{ts, mts, js, tsx, json}'",
    "analyze": "ANALYZE=true next build",
    "blog": "./article.sh",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "@vercel/og": "0.5.13",
    "feed": "4.2.2",
    "katex": "0.16.8",
    "mdast-util-to-hast": "12.3.0",
    "next": "13.4.19",
    "next-mdx-remote": "4.4.1",
    "next-themes": "0.2.1",
    "playwright": "1.37.1",
    "playwright-core": "1.37.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "7.0.0",
    "rehype-katex": "6.0.3",
    "rehype-mermaidjs": "1.0.1",
    "rehype-parse": "9.0.0",
    "rehype-pretty-code": "0.10.1",
    "rehype-raw": "6.1.1",
    "rehype-slug": "6.0.0",
    "remark-frontmatter": "4.0.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1",
    "remark-jaruby": "0.1.0",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.2",
    "remark-rehype": "10.1.0",
    "remark-slug": "7.0.1",
    "remark-stringify": "10.0.3",
    "remark-unwrap-images": "3.0.1",
    "sharp": "0.32.5",
    "shiki": "0.14.4",
    "strip-markdown": "5.0.1",
    "unified": "10.1.2",
    "unist-util-is": "6.0.0",
    "unist-util-visit": "5.0.0",
    "vfile": "6.0.1",
    "vfile-matter": "5.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.4.19",
    "@plaiceholder/next": "3.0.0",
    "@types/mdast": "4.0.0",
    "@types/mdx": "2.0.7",
    "@types/node": "20.6.0",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/unist": "3.0.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "fetch-site-metadata": "0.1.3",
    "globby": "13.2.2",
    "plaiceholder": "3.0.0",
    "postcss": "8.4.29",
    "postcss-import": "15.1.0",
    "postcss-nesting": "12.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
