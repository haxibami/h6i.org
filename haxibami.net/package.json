{
  "name": "haxibami.net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "ts-node --esm hooks/before-build.mts",
    "build": "next build",
    "postbuild": "ts-node --esm hooks/after-build.mts",
    "start": "next start",
    "pw-install": "playwright install chromium",
    "ts-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --check '**/*.{ts, tsx, json}'",
    "analyze": "ANALYZE=true next build",
    "blog": "./article.sh",
    "prepare": "cd .. && husky install haxibami.net/.husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js, ts, jsx, tsx, json}": [
      "prettier --write",
      "next lint --fix"
