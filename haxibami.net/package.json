{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "haxibami.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "ts-node --esm hooks/before-build.mts",
    "build": "next build",
    "postbuild": "ts-node --esm hooks/after-build.mts",
    "start": "next start",
    "pw-install": "playwright install chromium",
    "ts-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --check '**/*.{ts, mts, js, tsx, json}'",
    "analyze": "ANALYZE=true next build",
    "blog": "./article.sh",
    "prepare": "cd .. && husky install haxibami.net/.husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js, ts, jsx, tsx, json}": [
      "prettier --write",
      "next lint --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@next/bundle-analyzer": "13.1.1",
    "@next/font": "13.1.1",
    "@vercel/og": "0.0.22",
    "feed": "4.2.2",
    "image-size": "^1.0.2",
    "mdast-util-to-hast": "12.2.5",
    "mermaid": "9.3.0",
    "metadata-scraper": "0.2.61",
    "next": "13.1.1",
    "next-mdx-remote": "4.2.1",
    "next-themes": "0.2.1",
    "playwright": "1.29.2",
    "playwright-aws-lambda": "0.9.0",
    "playwright-core": "1.29.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-katex": "6.0.2",
    "rehype-parse": "8.0.4",
    "rehype-pretty-code": "0.7.0",
    "rehype-raw": "6.1.1",
    "rehype-slug": "5.1.0",
    "remark-frontmatter": "4.0.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1",
    "remark-jaruby": "0.1.0",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remark-slug": "7.0.1",
    "remark-stringify": "10.0.2",
    "remark-unwrap-images": "3.0.1",
    "shiki": "0.12.1",
    "strip-markdown": "5.0.0",
    "svgo": "3.0.2",
    "unified": "10.1.2",
    "unist-util-is": "5.1.1",
    "unist-util-visit": "4.1.1",
    "vfile": "5.3.6",
    "vfile-matter": "4.0.0"
  },
  "devDependencies": {
    "@plaiceholder/next": "^2.5.0",
    "@types/csso": "5.0.0",
    "@types/hast": "^2.3.4",
    "@types/mdast": "3.0.10",
    "@types/mdx": "2.0.3",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
